def printout(entry):
	for k, v in entry.items():
		print("{0:<15}".format(k.capitalize() + ": "), end="")
		# print(k, end=": ")
		if isinstance(v, list):
			L = [] # empty list
			for e in v:
				L.append(str(e))
			vls = ", ".join(L)
			print(vls)	
			del L # flush from memmory
			del vls # flush from memmory
		else:
			print(v)
	print()
