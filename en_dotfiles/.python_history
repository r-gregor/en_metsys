def sum_of_n(n):
	the_sum = 0
	for i in range (1, n+1):
		the_sum = the_sum + 1
	return the_sum
sum_of_n(10)
def sum_of_n(n):
	the_sum = 0
	for i in range (1, n+1):
		the_sum = the_sum + i
	return the_sum
