	print("{}\t{}: {}m\tteža: {}kg".format(i[0], i[1], i[2][0], i[2][1]))
for i in L:
	print("{}\t{}: {}m\tteža: {}kg".format(i[0], i[1], i[2][0], i[2][1]))
for i in L:
	print("{}\t{}: {}m\tteža: {}kg/m".format(i[0], i[1], i[2][0], i[2][1]))
for i in L:
	ms = i[2][0]
	tzm = i[2][1]
	tz = tzm * ms
	print("{}\t{}: {}m\tteža: {}kg/m\tSkupna teža: {}".format(i[0], i[1], ms, tzm, tz))
