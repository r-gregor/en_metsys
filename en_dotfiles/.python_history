def printout(entry):
	for k, v in entry.items():
		print("{0:<15}".format(k.capitalize() + ": "), end="")
		# print(k, end=": ")
		if isinstance(v, list):
			L = []
			for e in v:
				L.append(str(e))
			vls = ", ".join(L)
			print(vls)	
			del L
		else:
			vls = v
			print(vls)
			vls = ""
