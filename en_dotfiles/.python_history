	def __init__(self, name):
		self.name = name
	def add_trick(self, trick):
		self.tricks.append(trick)
d = Dog('Fido')
e = Dog('Buddy')
d.add_trick('roll over')
class Dog:
	def __init__(self, name):
		self.name = name
		self.tricks=[]
	def add_trick(self, trick):
		self.tricks.append(trick)
d = Dog('Fido')
e = Dog('Buddy')
