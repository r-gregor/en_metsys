class Queue:
	def __init__(self):
		self.items = []
q = Sueue()
q = Queue()
class Queue:
	def __init__(self):
		self.items = []
	
	def is_empty(self):
		return self.items == []
	
	def enqueue(self, item):
		self.items.insert(0, item)
	
	def dequeue(self):
		return self.items.pop()
	
	def size(self):
		return len(self.items)
q = Queue
q.enqueue('Gregor')
q.is_empty()
q
q.size()
q.size(self)
q.size()
q = Queue()
q.sizr()
q.size()
