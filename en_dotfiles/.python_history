for N in range(8):
	print("/" + " "*N + "\")
for N in range(8):
	print("/" + " "*N + "\\")
for N in range(8):
K = 8
for N in range(K):
	print(" "*K + "/" + " "*N + "\\")
	K += 1
for N in range(K):
	print(" "*K + "/" + " "*N + "\\")
